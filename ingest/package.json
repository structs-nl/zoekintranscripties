{
  "name": "transcripties-zoeken-tonen-ingest",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./build/index.js",
    "build": "rm -rf ./build && npm run generate:routes && npm run generate:schema && tsc",
    "gcp-build": "npm run build",
    "start:dev": "concurrently \"npm run dev\" \"onchange './src/**/*.ts' -e './src/routes/**/*' -- npm run generate:routes\"",
    "dev": "ELASTIC_URL=http://localhost:9200 ts-node-dev --max-old-space-size=4096 -r dotenv/config ./src/index.ts dotenv_config_path=../.env",
    "postdev": "npm run generate:routes",
    "generate:routes": "tsoa spec-and-routes",
    "generate:schema": "typescript-json-schema ./src/modules/upload/model.ts DocumentResponseLD --out ./src/schema.json",
    "test": "ELASTIC_URL=http://localhost:9200 jasmine-ts --config=jasmine.json",
    "test:watch": "nodemon --ext ts --exec 'jasmine-ts  --config=jasmine.json'",
    "generate:docs": "typedoc --out docs src",
    "start:docs": "npx serve docs",
    "coverage": "nyc npm run test"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.11.0",
    "@types/supertest": "^2.0.10",
    "ajv": "^7.0.0-rc.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "overload-protection": "^1.2.1",
    "swagger-ui-express": "^4.1.5",
    "tslib": "^2.0.1",
    "tsoa": "^3.4.0",
    "typescript": "^4.0.2",
    "typescript-json-schema": "^0.45.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.5.12",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/overload-protection": "^1.2.0",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "istanbul": "^0.4.5",
    "jasmine": "^3.6.1",
    "jasmine-node": "^3.0.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.0",
    "nyc": "^15.1.0",
    "onchange": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.59",
    "typedoc": "^0.20.35"
  }
}
