{
  "name": "transcripties-zoeken-tonen-api",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./build/index.js",
    "build": "rm -rf ./build && npm run generate:routes && tsc",
    "start:dev": "ELASTIC_URL=http://localhost:9200 concurrently \"npm run dev\" \"onchange './src/**/*.ts' -e './src/routes/*' -- npm run generate:routes\"",
    "dev": "ts-node-dev -r dotenv/config ./src/index.ts dotenv_config_path=../.env",
    "postdev": "npm run generate:routes",
    "test": "ELASTIC_URL=http://localhost:9200 jasmine-ts --config=jasmine.json",
    "coverage": "nyc npm run test",
    "generate:routes": "tsoa spec-and-routes",
    "generate:docs": "typedoc --out docs --exclude \"**/*+(index|.spec|.e2e).ts\" src",
    "start:docs": "npx serve docs",
    "audit": "npm audit",
    "antlr4ts": "antlr4ts -visitor src/grammar/ElasticsearchGrammar.g4"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "node-fetch": "^2.6.1",
    "swagger-ui-express": "^4.1.4",
    "tslib": "^2.0.1",
    "tsoa": "^3.5.2",
    "typescript": "^4.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.5.12",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "jasmine": "^3.7.0",
    "jasmine-node": "^3.0.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.3",
    "nyc": "^15.1.0",
    "onchange": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.59",
    "typedoc": "^0.20.35"
  }
}
